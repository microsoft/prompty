{
  "name": "@prompty/emitter",
  "version": "0.1.10",
  "license": "MIT",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "peerDependencies": {
    "@typespec/compiler": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@typespec/compiler": "latest",
    "eslint": "^9.15.0",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsc && cp -r ./src/templates ./dist/src",
    "watch": "tsc --watch",
    "generate": "tsp compile ../prompty/model/main.tsp --config ../prompty/tspconfig.yaml && markdownlint ../prompty/docs/* -f -c ./markdown.json",
    "generate:meai": "tsp compile ../prompty/model/main.tsp --config ../prompty/tspconfig-agent.yaml && markdownlint ./tsp-output/Markdown/* -f -c ./markdown.json",
    "test": "node --test",
    "lint": "eslint src/ test/ --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "format": "prettier .  --write",
    "format:check": "prettier --check ."
  },
  "private": false,
  "packageManager": "npm@11.5.1+sha512.232e6f5d9e799bcb486920b3e9ba907fdf96e576cf7e8c9446c8162e33a416096a1d37a9e910d9a918f6b1f606791c99bc6bb61ee2569b496ec74af13d0dbd95",
  "dependencies": {
    "nunjucks": "^3.2.4",
    "yaml": "^2.8.1"
  }
}